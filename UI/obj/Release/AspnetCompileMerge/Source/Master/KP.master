                                <tr>
                                    <td id="td2" runat="server" height="49" align="left" style="border-bottom: #314671 1px solid;
                                        border-right: #314671 1px solid; background-image: url(../images/new/backBar2.png);">
                                        <uc1:Buttons1 ID="Buttons1_1" runat="server" />
